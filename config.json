{
  "apiPackage": "apis",
  "modelPackage": "models",

  "supportsES6": true,
  "withInterfaces": true,
  "withSeparateModelsAndApi": true,
  "useSingleRequestParameter": true,
  "prependFormOrBodyParameters": true,
  "templateDir": "/local/typescript-axios",
  "files": {
    "api.mustache": {
      "templateType": "API",
      "destinationFilename": "apis.ts"
    },
    "model.mustache": {
      "templateType": "MODEL",
      "destinationFilename": "models.ts"
    }
  }
}
